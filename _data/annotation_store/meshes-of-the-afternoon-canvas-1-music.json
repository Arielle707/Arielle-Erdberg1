      }
    },
    {
      "@context": "http://www.w3.org/ns/anno.jsonld",
      "id": "meshes-of-the-afternoon-canvas-1-music-annotation-5.json",
      "type": "Annotation",
      "motivation": [
        "supplementing",
        "commenting"
      ],
      "body": {
        "type": "TextualBody",
        "value": "The guitars music is played slower and slower leading up to the woman setting foot into the house. As soon as her first foot crosses the threshold a cow bell or drumb is played. It is as if it is the heart beat of the house. Played in two at a time the notes give the house personality, almost as if it is alive.",
        "format": "text/plain",
        "purpose": "commenting"
      },
      "target": {
        "source": {
          "id": "https://Arielle707.github.io/Arielle-Erdberg1/meshes-of-the-afternoon/canvas-1/canvas",
          "type": "Canvas",
          "partOf": [
            {
              "id": "https://Arielle707.github.io/Arielle-Erdberg1/meshes-of-the-afternoon/manifest.json",
              "type": "Manifest"
            }
